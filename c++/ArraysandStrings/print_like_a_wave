//in this we have to print an 2d array like and way
//i.e 1st column downwards
//then other one upwards

void wavePrint(int input[][1000], int m, int n){
	
int rows=m,col=n;

for(int i=0;i<col;i++)
{
	for(int j=0;j<rows;j++)
	{
		if(i%2==0)
		{
			cout<<input[j][i]<<" ";
		}
		else{
			cout<<input[rows-1-j][i]<<" ";
		}
	}
}


}
